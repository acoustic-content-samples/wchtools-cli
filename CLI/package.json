  "main": "index.js",
  "bin": {
    "wchtools": "./bin/wchtools",
    "wchtools_heap": "./bin/wchtools_heap"
  },
  "dependencies": {
    "@acoustic-content-sdk/cli-credentials": "^9.0",
    "cli-table": "^0.3.1",
    "colors": "^1.3.2",
    "commander": "^2.19.0",
    "email-validator": "^2.0.4",
    "log4js": "^3.0.5",
    "ora": "^1.2.0",
    "prompt": "^1.0.0",
    "q": "^1.5.0",
    "request-promise-native": "^1.0.8",
    "rimraf": "^2.6.1",
    "rxjs": "^6",
    "update-notifier": "^4.1.0",
    "valid-url": "^1.0.9",
    "wchtools-api": "../authoring-api"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cp": "^0.2.0",
    "cpr": "^2.0.2",
    "diff": "^3.2.0",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "request": "^2.88.0",
    "sinon": "^1.10.3",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^2.10.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "unit": "./node_modules/.bin/istanbul cover ../node_modules/mocha/bin/_mocha test/unit/*.test.js -- -R spec",
