{
  "name": "wchtools-cli",
  "description": "Command line tools for Acoustic Content",
  "version": "4.5.3",
  "keywords": [
    "cli"
  ],
  "author": "Acoustic, L.P.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/acoustic-content-samples/wchtools-cli/issues"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "config": {
    "engine-strict": true
